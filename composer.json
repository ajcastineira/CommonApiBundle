{
    "name": "fresh/common-api-bundle",
    "description": "Provides some missing out-of-box features for RESTful API services based on Symfony.",
    "keywords": ["bundle", "symfony", "api", "boolean", "json decoder"],
    "homepage": "https://github.com/fre5h/CommonApiBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Genvald",
            "email": "genvaldartem@gmail.com",
            "role": "Creator"
        }
    ],
    "support": {
        "email": "genvaldartem@gmail.com",
        "issues": "https://github.com/fre5h/CommonApiBundle/issues"
    },
    "require": {
        "php": ">=5.6.0",
        "symfony/config": "^2.8.6 || ^3.0.6",
        "symfony/form": "^2.8.6 || ^3.0.6",
        "symfony/framework-bundle": "^2.8.6 || ^3.0.6",
        "symfony/http-kernel": "^2.8.6 || ^3.0.6",
        "symfony/http-foundation": "^2.8.6 || ^3.0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.6",
        "matthiasnoback/symfony-config-test": "2.0.*",
        "squizlabs/php_codesniffer": "~2.7",
        "escapestudios/symfony2-coding-standard": "~2.9"
    },
    "autoload": {
        "psr-4": {
            "Fresh\\CommonApiBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "ftrrtf/rollbar-bundle": "Allows to log exceptions and message to the Rollbar service",
        "sentry/sentry-symfony": "Allows to log exceptions and message to the Sentry service"
    }
}
